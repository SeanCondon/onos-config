ARG PLUGIN_BUILD_VERSION
FROM onosproject/onos-config-plugins:${PLUGIN_BUILD_VERSION} as pluginbase

FROM gcr.io/distroless/base-debian10
ARG PLUGIN_MAKE_TARGET
ARG PLUGIN_MAKE_VERSION
COPY --from=pluginbase /go/src/github.com/onosproject/onos-config/build/_output/${PLUGIN_MAKE_TARGET}.so.${PLUGIN_MAKE_VERSION} /
COPY --from=pluginbase /go/src/github.com/onosproject/onos-config/build/_output/copylibandstay /

CMD ["/copylibandstay"]
